local function JVTC_fake_script() -- Execute.REANScript 
	local script = Instance.new('LocalScript', Execute)

	script.Parent.MouseButton1Down:Connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/Empereanor/Overworld-Hub/main/Overworld%20Reanimation'))()
	end)
end
coroutine.wrap(JVTC_fake_script)()
