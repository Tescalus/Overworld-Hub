local function GPHZ_fake_script() -- BHome.LocalScript 
	local script = Instance.new('LocalScript', BHome)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Home.Visible = true
		script.Parent.Parent.Scripts.Visible = false
		script.Parent.Parent.Credits.Visible = false
	end)
end
coroutine.wrap(GPHZ_fake_script)()
